{
  "name": "sa-ui",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.2",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "dev": "vite --config vitest.config.ts"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@sa-ui/constants": "workspace:*",
    "@sa-ui/utils": "workspace:*",
    "@vueuse/core": "9.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@esbuild-kit/cjs-loader": "2.3.3",
    "@types/jsdom": "20.0.0",
    "@types/node": "*",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "@vitest/ui": "0.23.4",
    "@vue/test-utils": "2.0.2",
    "@vue/tsconfig": "0.1.3",
    "eslint": "8.24.0",
    "eslint-define-config": "1.7.0",
    "expect-type": "0.14.2",
    "fast-glob": "3.2.12",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "resize-observer-polyfill": "1.5.1",
    "tsx": "3.9.0",
    "typescript": "4.8.4",
    "unplugin-vue-define-options": "0.11.2",
    "vitest": "0.23.4",
    "vue": "3.2.39",
    "vue-tsc": "0.40.13"
  },
  "engines": {
    "node": ">= 16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "react",
        "react-dom"
      ]
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
